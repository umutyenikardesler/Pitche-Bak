{
  "expo": {
    "name": "Pitche-Bak",
    "slug": "Pitche-Bak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.tumurelsedrakiney.pitchebak",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Yakınınızdaki sahaları göstermek ve konuma göre sıralamak için konumunuza ihtiyaç var."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC9VGm56IFnVAYEYKoPnPt3eGJ3_NMwPB4"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tumurelsedrakiney.PitcheBak"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcaeff72-7e2a-4511-8ad1-73798a478d1a"
      }
    }
  }
}
